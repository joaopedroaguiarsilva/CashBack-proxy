{
  "name": "cashback-proxy",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "auth": "xsuaa",
      "SalesOrder": {
        "kind": "odata-v2",
        "model": "srv/external/SalesOrder",
        "credentials": {
          "destination": "S4H_HTTP",
          "path": "/sap/opu/odata/sap/API_SALES_ORDER_SRV"
        }
      },
      "BusinessPartner": {
        "kind": "odata-v2",
        "model": "srv/external/BusinessPartner",
        "credentials": {
          "destination": "S4H_HTTP",
          "path": "/sap/opu/odata/sap/API_BUSINESS_PARTNER"
        }
      },
      "Product": {
        "kind": "odata-v2",
        "model": "srv/external/BusinessPartner",
        "credentials": {
          "destination": "S4H_HTTP",
          "path": "/sap/opu/odata/sap/API_PRODUCT_SRV"
        }
      }
    }
  }
}
